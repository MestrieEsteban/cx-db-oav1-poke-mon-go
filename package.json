{
  "name": "typescript-starter",
  "version": "1.0.0",
  "description": "Pokedex",
  "main": "main.ts",
  "scripts": {
    "dev": "nodemon",
    "clean": "yarn rimraf .dist",
    "build": "yarn clean && yarn babel src/ --out-dir .dist/ --extensions '.ts' --quiet",
    "start": "yarn build && node .dist/server",
    "lint": "eslint . --ext .ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier-format && yarn lint"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/lodash": "^4.14.161",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^12.7.2",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "lodash": "^4.17.20",
    "nodemon": "^1.19.1",
    "onchange": "^6.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "@types/request": "^2.48.5",
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "json-pokemon": "^1.0.0",
    "mongoose": "^5.10.8",
    "request": "^2.88.2"
  }
}
